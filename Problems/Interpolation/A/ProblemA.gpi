set term svg background rgb "white"
set out "PlotA.svg"
set multiplot layout 2,1
set tics out



set title "Spline"
plot "xyData.txt"  title "Created data from x^2-x^3" \
, "linterpData.txt"  title "linear splined data"


set title "Integration"
plot "integrationData.txt" u 1:3 t "x^3/3-x^4/4" w l \
,"integrationData.txt" u 1:2 t "Integration of x^3/3-x^4/4" 
unset key
